<Window x:Class="AutowashQueue.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Margin="140">
            <Button Content="Load Image" Click="LoadImgButton_click" x:Name="LoadImgButton"></Button>
            <Button Content="LookingAtQueue" Click="LookAtQueue_click" x:Name="LookAtQueue"></Button>
        </StackPanel>
        <Button Content="Exit" Click="Exit_click" Margin="410,290,10,10"></Button>
    </Grid>
</Window>
